{
  "name": "tracks-manager-client",
  "homepage": "https://genesiseducationkyiv.github.io/front-end-school-3-0-OlhaRyzha/",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "start": "npm run dev",
    "build": "vite build --outDir dist",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "clean:maps": "find build -name \"*.map\" -type f -delete",
    "predeploy": "npm run build && npm run clean:maps && cp build/index.html build/404.html",
    "deploy": "gh-pages -d build",
    "preview": "vite preview --port=4173",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ct": "playwright test --config=playwright.ct.config.ts",
    "test:e2e": "playwright test tests/e2e",
    "show-report": "playwright show-report --port=9324",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "playwright test",
    "unimported": "unimported",
    "docker:down": "docker-compose down",
    "docker:frontend": "export VITE_API_BASE_URL=http://localhost:8000/api && docker-compose up --build -d --no-deps frontend",
    "docker:backend": "docker-compose up -d backend",
    "docker:health:fe": "curl --fail http://localhost:8080/tracker/ && echo \"Frontend OK\"",
    "docker:health:be": "curl --fail http://localhost:8000/health && echo \"Backend OK\"",
    "docker:build": "docker buildx build --platform linux/amd64,linux/arm64 -t tracks-manager-client:latest .",
    "docker:tag": "docker tag tracks-manager-client:latest tracks-manager-client:${npm_package_version}",
    "docker:push": "docker push tracks-manager-client:latest",
    "docker:run": "docker run --rm -p 8080:80 tracks-manager-client:latest",
    "docker:ps": "docker ps -a",
    "docker:stopall": "docker stop $(docker ps -q)",
    "docker:rmall": "docker rm $(docker ps -aq)"
  },
  "dependencies": {
    "@mobily/ts-belt": "^3.13.1",
    "@radix-ui/react-alert-dialog": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.2.2",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-toast": "^1.2.11",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@reduxjs/toolkit": "^2.7.0",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/react-query-devtools": "^5.81.2",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.0",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "ldrs": "^1.1.7",
    "lucide-react": "^0.525.0",
    "qs": "^6.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.2",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "tailwind-merge": "^3.3.1",
    "wavesurfer.js": "^7.9.4",
    "zod": "^3.25.76",
    "zod-formik-adapter": "^1.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@ljharb/tsconfig": "^0.3.2",
    "@playwright/experimental-ct-react": "^1.53.1",
    "@playwright/test": "^1.53.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^24.0.3",
    "@types/qs": "^6.9.18",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-persist": "^4.3.1",
    "@types/testing-library__user-event": "^4.2.0",
    "@types/wavesurfer.js": "^6.0.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^4.1.4",
    "gh-pages": "^6.3.0",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "path": "^0.12.7",
    "playwright": "^1.53.1",
    "postcss": "^8.5.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "ts-prune": "^0.10.3",
    "typescript": "~5.8.3",
    "unimported": "^1.31.1",
    "vite": "^7.0.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-purgecss": "^0.2.13",
    "vitest": "^3.2.4"
  }
}
